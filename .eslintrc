{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "comma-dangle": 0,
    "quotes": [ 2, "single" ],
    "no-undef": 2,
    "global-strict": 0,
    "no-extra-semi": 2,
    "no-underscore-dangle": 0,
    "no-console": 2,
    "no-unused-vars": 2,
    "no-trailing-spaces": [2, { "skipBlankLines": true }],
    "no-unreachable": 2,
    "no-alert": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-multi-spaces": 2,
    "brace-style": 2,
    "react/display-name": 0,
    "react/no-render-return-value": 0,
    "react/no-direct-mutation-state": 0
  }
}
